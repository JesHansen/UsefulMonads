<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>UsefulMonads</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>UsefulMonads</PackageId>
    <Authors>Jes Bak Hansen</Authors>
    <Product>Monads for .NET</Product>
    <Description>A collection of actually useful monads for .NET</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>monad either maybe</PackageTags>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <nullable>enable</nullable>
  </PropertyGroup>
</Project>
